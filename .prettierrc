{
  "plugins": [
    "./node_modules/prettier-plugin-svelte",
    "./node_modules/prettier-plugin-astro"
  ],
  "svelteBracketNewLine": true,
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": true,
  "embeddedLanguageFormatting": "auto",
  "printWidth": 100,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
